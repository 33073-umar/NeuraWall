[ req ]
default_bits       = 2048
prompt             = no
default_md         = sha256
distinguished_name = dn
req_extensions     = v3_req

[ dn ]
C  = US
ST = State
L  = City
O  = LocalDev
OU = IT
CN = 192.168.1.14      # <-- your serverâ€™s IP

[ v3_req ]
subjectAltName = @alt_names

[ alt_names ]
IP.1 = 192.168.1.14    # <-- include your server IP here

